<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="360dp"
    android:height="180dp"
    android:viewportWidth="360"
    android:viewportHeight="360"
    android:scaleType="fitXY"
    >

<group>

    <clip-path
        android:pathData="M0 0H360V629H0V0Z"
        />

    <group
        android:translateX="0"
        android:translateY="0"
        android:pivotX="0"
        android:pivotY="0"
        android:scaleX="1"
        android:scaleY="1"
        >

        <path
            android:pathData="M0 0V360H360V0"
            >

            <aapt:attr name="android:fillColor">

<gradient
    android:type="linear"
    android:startX="0"
    android:startY="0"
    android:endX="360"
    android:endY="0"
    >

    <item
        android:color="#4D799E"
        android:offset="0"
        />

    <item
        android:color="#394F61"
        android:offset="0.835"
        />

</gradient>

    </aapt:attr>

    </path>

    </group>

    </group>

    </vector>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape
            android:shape="line">
            <stroke
                android:color="#333353"
                android:width="18dp"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip android:drawable="@color/lightDarkPink">

        </clip>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape
                android:shape="line">
                <stroke
                    android:color="#9808"
                    android:width="20dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
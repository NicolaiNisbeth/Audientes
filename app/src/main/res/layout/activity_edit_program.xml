        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.238" />

    <SeekBar
        android:id="@+id/medium"
        android:layout_width="249dp"
        android:layout_height="40dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="428dp"
        android:elevation="3dp"
        android:max="100"
        android:progress="1"
        android:progressDrawable="@drawable/custom_seekbar"
        android:rotation="270"
        android:thumb="@drawable/custom_seekbar_slider"
        android:thumbOffset="10dp"
        android:splitTrack="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.276"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.238" />

    <SeekBar
        android:id="@+id/high"
        android:layout_width="249dp"
        android:layout_height="40dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="428dp"
        android:elevation="3dp"
        android:max="100"
        android:progress="1"
        android:progressDrawable="@drawable/custom_seekbar"
        android:rotation="270"
        android:thumb="@drawable/custom_seekbar_slider"
        android:thumbOffset="10dp"
        android:splitTrack="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.238" />

    <SeekBar
        android:id="@+id/high_plus"
        android:layout_width="249dp"
        android:layout_height="40dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="428dp"
        android:elevation="3dp"
        android:max="100"
        android:progress="1"
        android:progressDrawable="@drawable/custom_seekbar"
        android:rotation="270"
        android:thumb="@drawable/custom_seekbar_slider"
        android:thumbOffset="10dp"
        android:splitTrack="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.238" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="291dp"
        android:layout_height="48dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="120dp"
        android:background="#6B5B6080"
        android:ems="10"
        android:hint="Name of Program"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/low_plus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/set_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="335dp"
        android:background="#0032385C"
        android:text="0"
        android:textColor="#EBEAEA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"

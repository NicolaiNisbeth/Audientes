<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/darkBlue"
    tools:context=".EditProgram">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="534dp"
        android:layout_height="209dp"
        android:layout_marginBottom="650dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/xml_bg_topmenu" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="159dp"
        android:layout_height="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.136"
        app:srcCompat="@drawable/xml_button_monka_fucking_s" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="289dp"
        android:layout_height="3dp"
        android:layout_marginTop="71dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207"
        app:srcCompat="@drawable/xml_button_monka_fucking_s" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Configuration"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.069" />

    <SeekBar
        android:id="@+id/low_plus"
        android:layout_width="249dp"
        android:layout_height="40dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="310dp"
        android:layout_marginBottom="428dp"
        android:elevation="3dp"
        android:max="100"
        android:progress="1"
        android:progressDrawable="@drawable/custom_seekbar"
        android:rotation="270"
        android:thumb="@drawable/custom_seekbar_slider"
        android:thumbOffset="10dp"
        android:splitTrack="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.576"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.238" />

    <SeekBar
        android:id="@+id/low"
        android:layout_width="249dp"
        android:layout_height="40dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="325dp"
        android:layout_marginBottom="428dp"
        android:elevation="3dp"
        android:max="100"
        android:progress="1"
        android:progressDrawable="@drawable/custom_seekbar"
        android:rotation="270"
        android:thumb="@drawable/custom_seekbar_slider"
        android:thumbOffset="10dp"
        android:splitTrack="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

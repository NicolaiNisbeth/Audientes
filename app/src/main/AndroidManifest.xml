<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="a3.audientes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".EditProgram"
            android:theme="@style/AppTheme.ActionBar.Light" />
        <activity android:name=".fragment_edit_popup" />
        <activity android:name=".activities.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.HearingProfile"
            android:theme="@style/AppTheme.ActionBar.Dark"/>
        <activity android:name=".fragments.StartHearingTest" />

        <activity android:name=".fragments.HearingTest" />
    </application>

</manifest>